<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.east.demo.persist.mapper.LyUserInfoMapper">

    <!-- 通过name查询 -->
    <select id="getByName" resultType="com.east.demo.persist.entity.LyUserInfo">
        <![CDATA[
        SELECT * FROM HR WHERE name = #{value} and rownum = 1
        ]]>
    </select>

</mapper>